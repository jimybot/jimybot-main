{
  "name": "music-pattern-webapp",
  "version": "0.0.1",
  "private": true,
  "homepage": "/",
  "engines": {
    "node": ">=12.13.0 <13.0.0"
  },
  "dependencies": {
    "@ibm/plex": "^4.0.1",
    "line-awesome": "^1.3.0",
    "lodash.capitalize": "^4.2.1",
    "lodash.debounce": "^4.0.8",
    "prop-types": "15.7.2",
    "re-reselect": "^3.4.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-dropzone": "^10.2.1",
    "react-helmet": "5.2.1",
    "react-leaflet": "2.6.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "redux": "4.0.4",
    "redux-persist": "6.0.0",
    "redux-react-modals": "1.3.0",
    "redux-thunk": "2.3.0",
    "redux-thunk-data": "1.6.4",
    "reselect": "^4.0.0",
    "whatwg-fetch": "^3.0.0",
    "with-react-query": "^1.1.2",
    "with-react-redux-login": "1.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "cypress": "^3.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "jest-environment-jsdom": "24.9.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "license-reporter": "^1.2.2",
    "node-sass": "^4.13.0",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.3"
  },
  "resolutions": {
    "react-helmet/react-side-effect": "2.1.0"
  },
  "scripts": {
    "build": "scripts/build.sh",
    "eject": "react-scripts eject",
    "format:js": "prettier-eslint --write '{src,tests}/**/*.{js,jsx}'",
    "licenses": "node_modules/.bin/license-reporter save --json licenses.json",
    "postinstall": "mkdir -p src/styles/fonts/IBM-Plex-Sans/fonts/complete && cp -rp node_modules/@ibm/plex/IBM-Plex-Sans/fonts/complete/woff* src/styles/fonts/IBM-Plex-Sans/fonts/complete",
    "start": "scripts/start.sh",
    "test:end2end": "node_modules/.bin/cypress run",
    "test:frontend": "jest --env=jsdom"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/hooks/precommit.sh"
    }
  }
}
